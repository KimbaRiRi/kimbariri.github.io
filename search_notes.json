---
layout: null
---

[
  {% for post in site.notes %}
    {

      "title"    : "{{ post.title | strip_html | escape }}",
      "url"      : "{{ post.url | remove: "/"}}",
      "category" : "{{post.categories | join: ', '}}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "content"  : {{ post.content | strip_html | strip_newline | strip | jsonify }}

    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]